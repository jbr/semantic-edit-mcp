{
    "type": "object",
    "properties": {
        "file_path": {
            "type": "string",
            "description": "Path to the Rust source file (.rs) - RUST FILES ONLY"
        },
        "content": {
            "type": "string",
            "description": "Content to insert at module level"
        },
        "position": {
            "type": "string",
            "description": "Where to insert in the module",
            "enum": ["start", "end"],
            "default": "end"
        },
        "preview_only": {
            "type": "boolean",
            "description": "If true, show preview of changes without writing to file",
            "default": false
        }
    },
    "required": ["file_path", "content"],
    "additionalProperties": false
}
